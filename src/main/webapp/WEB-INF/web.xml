<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>RegistrarServlet</servlet-name>
        <servlet-class>pe.edu.servlets.RegistrarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PartInsServlet</servlet-name>
        <servlet-class>pe.edu.servlets.PartInsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelarServlet</servlet-name>
        <servlet-class>pe.edu.servlets.CancelarServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrarServlet</servlet-name>
        <url-pattern>/RegistrarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PartInsServlet</servlet-name>
        <url-pattern>/PartInsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelarServlet</servlet-name>
        <url-pattern>/CancelarServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
